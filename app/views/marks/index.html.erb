<div class="container">
<div class="breadcumb">
    <li><%=link_to "Home",root_path%> | Grades</li>
  </div>
<div class="container" >
  <% @users.each do |user| %>
  <%if !user.teacher && !user.admin%>
  
  
  <table class="table table-hover">
  <tr>
  <td><h5> <%= link_to user.name+" "+user.surname, user %></h5></td>
  <% user.marks.where(:subject_id =>@subject).map do |mark| %>
    <td><%=  mark.value%></td>
  
  <%end%>

  <td>  <%= render 'marks/form', user: user %></td>
  </tr>
  </table>
  <%end%>
  <%end%>
</div>
</div>